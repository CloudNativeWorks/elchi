{
    "compilerOptions": {
        "target": "ES2020",
        "module": "CommonJS",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "strict": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "allowJs": true,
        "outDir": "./dist"
    },
    "include": [
        "../fieldsToTags/**/*"
    ],
    "exclude": [
        "node_modules"
    ]
}